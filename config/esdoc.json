{
  "source": "./src",
  "destination": "./docs",
  "plugins": [

    {"name": "esdoc-es7-plugin"},

    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^src/", "to": ""},
          {"from": "\\.js$", "to": ""},
          {"from": "/index$", "to": ""}
        ]
      }
    }

  ]
}
