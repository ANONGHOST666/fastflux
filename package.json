{
  "name": "fastflux",
  "version": "0.1.17",
  "description": "Message driven reactive state management",
  "keywords": [
    "flux",
    "frp",
    "functional",
    "redux",
    "react",
    "reactive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rvikmanis/fastflux.git"
  },
  "main": "./index",
  "author": "Rudolfs Vikmanis <rudolfs.vikmanis@gmail.com> (https://github.com/rvikmanis)",
  "license": "MIT",
  "dependencies": {
    "object-assign": "^4.0.1",
    "react": "^0.14.1"
  },
  "scripts": {
    "test": "jasmine",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build:node": "rollup -c rollup.config.node.js > index.js",
    "build:browser": "mkdir -p dist && rollup -c rollup.config.browser.js > dist/fastflux.min.js",
    "build": "npm run build:node && npm run build:browser",
    "clean": "rm -rf dist index.js",
    "docs:generate": "esdoc -c config/esdoc.json",
    "docs:clean": "rm -rf docs/html"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "cheerio": "^0.19.0",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-importpath-plugin": "0.0.1",
    "jasmine-es6": "^0.1.1",
    "mock-browser": "^0.91.34",
    "react-dom": "^0.14.1",
    "rollup": "^0.25.1",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.0",
    "rollup-plugin-npm": "^1.3.0",
    "rollup-plugin-uglify": "^0.1.0"
  }
}
